program = statement* EOF

statement = print_stmt
print_stmt = "print" expression ";"

expression = term
term = factor (("+" | "-") term)*
factor = primary (("*" | "/") factor)*
primary = number | "(" expression ")"

identifier = alpha (alpha | digit)*
number = digit+ ("." digit+)?
string = "<anything except ">"
bool = "true" | "false"

alpha = "_" | upper | lower
upper = "A..Z"
lower = "a..z"
digit = "0..9"
