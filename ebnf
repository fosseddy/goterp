program = statement* EOF

statement = print_stmt
print_stmt = "print" expression ";"

expression = term
term = unary (("+"|"-") unary)*
unary = "-" unary | primary
primary = number | "(" expression ")"

number = digit+ ("." digit+)?

alpha = "_"|upper|lower
upper = "A..Z"
lower = "a..z"
digit = "0..9"
