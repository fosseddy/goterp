program = statement* EOF

statement = print_stmt
print_stmt = "print" expression ";"

expression = term
term = factor (("+" | "-") factor)*
factor = unary (("*" | "/") unary)*
unary = "-" unary | primary
primary = number | bool | "(" expression ")"

number = digit+ ("." digit+)?
bool = "true" | "false"

alpha = "_" | upper | lower
upper = "A..Z"
lower = "a..z"
digit = "0..9"
